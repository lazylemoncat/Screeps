{"code":"import { roleHarvester } from './Role/RoleHarvester';\r\nimport { roleBuilder } from './Role/RoleBuilder';\r\nimport { roleUpgrader } from './Role/RoleUpgrader';\r\nimport { roleAttacker } from './Role/RoleAttacker';\r\nimport { roleHealer } from './Role/RoleHealer';\r\nimport { roleRepairer } from './Role/RoleRepairer';\r\nimport { roleTransfer } from './Role/RoleTransfer';\r\nimport { roleClaimer } from './Role/RoleClaimer';\r\nimport { structureTower } from './Structure/StructureTower';\r\nimport { newCreeps } from './NewCreep/NewCreeps';\r\nconst { refreshGlobal } = await import('./global/RefreshGlobal');\r\nexport const loop = function () {\r\n    if (Game.spawns.Spawn1 == undefined) {\r\n        return;\r\n    }\r\n    if (Game.time % 100 == 0) {\r\n        refreshGlobal();\r\n    }\r\n    // create new creeps\r\n    newCreeps.run();\r\n    // run creeps\r\n    for (let name in Game.creeps) {\r\n        let creep = Game.creeps[name];\r\n        switch (creep.memory.role) {\r\n            case 'harvester':\r\n                roleHarvester.run(creep);\r\n                break;\r\n            case 'upgrader':\r\n                roleUpgrader.run(creep);\r\n                break;\r\n            case 'builder':\r\n                roleBuilder.run(creep);\r\n                break;\r\n            case 'transfer':\r\n                roleTransfer.run(creep);\r\n                break;\r\n            case 'repairer':\r\n                roleRepairer.run(creep);\r\n                break;\r\n            case 'attacker':\r\n                roleAttacker.run(creep);\r\n                break;\r\n            case 'healer':\r\n                roleHealer.run(creep);\r\n                break;\r\n            case 'claimer':\r\n                roleClaimer.run(creep);\r\n                break;\r\n        }\r\n    }\r\n    // run structures\r\n    for (let name in Game.structures) {\r\n        let structure = Game.structures[name];\r\n        switch (structure.structureType) {\r\n            case STRUCTURE_TOWER: structureTower.run(structure);\r\n        }\r\n    }\r\n};\r\n//# sourceMappingURL=main.js.map","references":["D:/Desktop/ScreepsCodes/World/src/Role/RoleHarvester.ts","D:/Desktop/ScreepsCodes/World/src/Role/RoleBuilder.ts","D:/Desktop/ScreepsCodes/World/src/Role/RoleUpgrader.ts","D:/Desktop/ScreepsCodes/World/src/Role/RoleAttacker.js","D:/Desktop/ScreepsCodes/World/src/Role/RoleHealer.js","D:/Desktop/ScreepsCodes/World/src/Role/RoleRepairer.ts","D:/Desktop/ScreepsCodes/World/src/Role/RoleTransfer.ts","D:/Desktop/ScreepsCodes/World/src/Role/RoleClaimer.ts","D:/Desktop/ScreepsCodes/World/src/Structure/StructureTower.ts","D:/Desktop/ScreepsCodes/World/src/NewCreep/NewCreeps.ts","D:/Desktop/ScreepsCodes/World/src/global/RefreshGlobal.ts"],"map":"{\"version\":3,\"file\":\"main.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../src/main.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,aAAa,EAAE,MAAM,sBAAsB,CAAC;AACrD,OAAO,EAAE,WAAW,EAAE,MAAM,oBAAoB,CAAC;AACjD,OAAO,EAAE,YAAY,EAAE,MAAM,qBAAqB,CAAC;AACnD,OAAO,EAAE,YAAY,EAAE,MAAM,qBAAqB,CAAC;AACnD,OAAO,EAAE,UAAU,EAAE,MAAM,mBAAmB,CAAC;AAC/C,OAAO,EAAE,YAAY,EAAE,MAAM,qBAAqB,CAAC;AACnD,OAAO,EAAE,YAAY,EAAE,MAAM,qBAAqB,CAAC;AACnD,OAAO,EAAE,WAAW,EAAE,MAAM,oBAAoB,CAAC;AAEjD,OAAO,EAAE,cAAc,EAAE,MAAM,4BAA4B,CAAC;AAE5D,OAAO,EAAE,SAAS,EAAE,MAAM,sBAAsB,CAAC;AAEjD,MAAM,EAAE,aAAa,EAAE,GAAG,MAAM,MAAM,CAAC,wBAAwB,CAAC,CAAC;AAEjE,MAAM,CAAC,MAAM,IAAI,GAAG;IAClB,IAAI,IAAI,CAAC,MAAM,CAAC,MAAM,IAAI,SAAS,EAAE;QACnC,OAAO;KACR;IACD,IAAI,IAAI,CAAC,IAAI,GAAG,GAAG,IAAI,CAAC,EAAE;QACxB,aAAa,EAAE,CAAC;KACjB;IACD,oBAAoB;IACpB,SAAS,CAAC,GAAG,EAAE,CAAA;IACf,aAAa;IACb,KAAK,IAAI,IAAI,IAAI,IAAI,CAAC,MAAM,EAAE;QAC5B,IAAI,KAAK,GAAU,IAAI,CAAC,MAAM,CAAC,IAAI,CAAC,CAAC;QACrC,QAAQ,KAAK,CAAC,MAAM,CAAC,IAAI,EAAE;YACzB,KAAK,WAAW;gBAAG,aAAa,CAAC,GAAG,CAAC,KAAK,CAAC,CAAC;gBAAC,MAAM;YACnD,KAAK,UAAU;gBAAG,YAAY,CAAC,GAAG,CAAC,KAAK,CAAC,CAAC;gBAAC,MAAM;YACjD,KAAK,SAAS;gBAAG,WAAW,CAAC,GAAG,CAAC,KAAK,CAAC,CAAC;gBAAC,MAAM;YAC/C,KAAK,UAAU;gBA